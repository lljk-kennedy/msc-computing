import java.util.ArrayList;

public class MySet {
	
	private int[] array;
		
	public MySet(int[] newArray)
	{
		array = newArray;
	}
	
	public Boolean setSize(int sizeToSet)
	{
		array = new int[sizeToSet];
		return true;
	}
	
	public Boolean changeElement(int elementToSet, int valToSet)
	{
		if (elementToSet >= 0 && elementToSet < array.length)
		{
			if (checkForUniqueness(valToSet))
			{
				array[elementToSet] = valToSet;
				return true;
			}
			else return false;
		}
		else return false;
	}
	
	public int getElement(int element)
	{
		return array[element];
	}
	
	public Boolean checkForUniqueness(int valToSet)
	{
		for (int i=0; i < array.length; i++)
		{
			if (array[i] == valToSet)
					return false;
		}
		
		return true;
	}
	
	public int length()
	{
		return array.length;
	}
		
	public Boolean isMember(int valToCheck)
	{
		for (int i = 0; i < array.length; i++)
		{
			if (valToCheck == array[i])
					return true;
		}
		return false;
	}
	
	public static Boolean isMember(int valToCheck, MySet setToCheck)
	{
		for (int i = 0; i < setToCheck.length(); i++)
		{
			if (valToCheck == setToCheck.getElement(i))
					return true;
		}
		return false;
	}
	
	public static MySet Union(MySet set1, MySet set2)
	{
		ArrayList<Integer> unionSet = new ArrayList<Integer>();
		
		for (int i = 0; i < set1.length(); i++)
		{
			if (MySet.isMember(set1.getElement(i), set2))
				unionSet.add(set1.getElement(i));
			
		}
		
	
	}
}